class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        x = ""
        
        for i in range(len(s)):
            if s[i].isalnum():
                x += s[i].lower()
        
        value = x == x[::-1]
        
        return value
        

        
